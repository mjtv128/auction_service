curl --location --request POST 'https://dev-7l8o9x-z.us.auth0.com/oauth/token' \
--header 'Content-Type: application/x-www-form-urlencoded' \
--data-urlencode 'client_id=S01ZJsmhPZtxZZaNYvHLqB5Sd7piudi5' \
--data-urlencode 'username=mjtvan@gmail.com' \
--data-urlencode 'password=Password123' \
--data-urlencode 'grant_type=password' \
--data-urlencode 'scope=openid'

